void CalcPixelMaterialInputs(in out FMaterialPixelParameters Parameters, in out FPixelMaterialInputs PixelMaterialInputs)
{
	//WorldAligned texturing & others use normals & stuff that think Z is up
	Parameters.TangentToWorld[0] = Parameters.TangentToWorld[0].xzy;
	Parameters.TangentToWorld[1] = Parameters.TangentToWorld[1].xzy;
	Parameters.TangentToWorld[2] = Parameters.TangentToWorld[2].xzy;

	float3 WorldNormalCopy = Parameters.WorldNormal;

	// Initial calculations (required for Normal)

	// The Normal is a special case as it might have its own expressions and also be used to calculate other inputs, so perform the assignment here
	PixelMaterialInputs.Normal = MaterialFloat3(0.21223100,0.21223100,1.00000000);


#if TEMPLATE_USES_STRATA
	Parameters.StrataPixelFootprint = StrataGetPixelFootprint(Parameters.WorldPosition_CamRelative, GetRoughnessFromNormalCurvature(Parameters));
	Parameters.SharedLocalBases = StrataInitialiseSharedLocalBases();
	Parameters.StrataTree = GetInitialisedStrataTree();
#endif

	// Note that here MaterialNormal can be in world space or tangent space
	float3 MaterialNormal = GetMaterialNormal(Parameters, PixelMaterialInputs);

#if MATERIAL_TANGENTSPACENORMAL

#if FEATURE_LEVEL >= FEATURE_LEVEL_SM4
	// Mobile will rely on only the final normalize for performance
	MaterialNormal = normalize(MaterialNormal);
#endif

	// normalizing after the tangent space to world space conversion improves quality with sheared bases (UV layout to WS causes shrearing)
	// use full precision normalize to avoid overflows
	Parameters.WorldNormal = TransformTangentNormalToWorld(Parameters.TangentToWorld, MaterialNormal);

#else //MATERIAL_TANGENTSPACENORMAL

	Parameters.WorldNormal = normalize(MaterialNormal);

#endif //MATERIAL_TANGENTSPACENORMAL

#if MATERIAL_TANGENTSPACENORMAL
	// flip the normal for backfaces being rendered with a two-sided material
	Parameters.WorldNormal *= Parameters.TwoSidedSign;
#endif

	Parameters.ReflectionVector = ReflectionAboutCustomWorldNormal(Parameters, Parameters.WorldNormal, false);

#if !PARTICLE_SPRITE_FACTORY
	Parameters.Particle.MotionBlurFade = 1.0f;
#endif // !PARTICLE_SPRITE_FACTORY

	// Now the rest of the inputs
	MaterialFloat3 Local0 = lerp(Material.PreshaderBuffer[3].xyz,Material.PreshaderBuffer[4].xyz,Material.PreshaderBuffer[3].w);
	FLWCVector3 Local1 = GetWorldPosition_NoMaterialOffsets(Parameters);
	FLWCVector3 Local2 = LWCMultiply(DERIV_BASE_VALUE(Local1), LWCPromote(((MaterialFloat3)Material.PreshaderBuffer[7].w)));
	FLWCVector2 Local3 = MakeLWCVector(LWCGetX(DERIV_BASE_VALUE(Local2)), LWCGetZ(DERIV_BASE_VALUE(Local2)));
	MaterialFloat2 Local4 = LWCApplyAddressMode(DERIV_BASE_VALUE(Local3), LWCADDRESSMODE_WRAP, LWCADDRESSMODE_WRAP);
	MaterialFloat Local5 = MaterialStoreTexCoordScale(Parameters, Local4, 3);
	MaterialFloat4 Local6 = Texture2DSample(Material_Texture2D_0,GetMaterialSharedSampler(samplerMaterial_Texture2D_0,View_MaterialTextureBilinearWrapedSampler),Local4);
	MaterialFloat Local7 = MaterialStoreTexSample(Parameters, Local6, 3);
	FLWCVector2 Local8 = MakeLWCVector(LWCGetY(DERIV_BASE_VALUE(Local2)), LWCGetZ(DERIV_BASE_VALUE(Local2)));
	MaterialFloat2 Local9 = LWCApplyAddressMode(DERIV_BASE_VALUE(Local8), LWCADDRESSMODE_WRAP, LWCADDRESSMODE_WRAP);
	MaterialFloat Local10 = MaterialStoreTexCoordScale(Parameters, Local9, 3);
	MaterialFloat4 Local11 = Texture2DSample(Material_Texture2D_0,GetMaterialSharedSampler(samplerMaterial_Texture2D_0,View_MaterialTextureBilinearWrapedSampler),Local9);
	MaterialFloat Local12 = MaterialStoreTexSample(Parameters, Local11, 3);
	MaterialFloat Local13 = abs(Parameters.TangentToWorld[2].r);
	MaterialFloat Local14 = lerp((0.00000000 - 1.00000000),(1.00000000 + 1.00000000),Local13);
	MaterialFloat Local15 = saturate(Local14);
	MaterialFloat3 Local16 = lerp(Local6.rgb,Local11.rgb,Local15.r.r);
	FLWCVector2 Local17 = MakeLWCVector(LWCGetX(DERIV_BASE_VALUE(Local2)), LWCGetY(DERIV_BASE_VALUE(Local2)));
	MaterialFloat2 Local18 = LWCApplyAddressMode(DERIV_BASE_VALUE(Local17), LWCADDRESSMODE_WRAP, LWCADDRESSMODE_WRAP);
	MaterialFloat Local19 = MaterialStoreTexCoordScale(Parameters, Local18, 3);
	MaterialFloat4 Local20 = Texture2DSample(Material_Texture2D_0,GetMaterialSharedSampler(samplerMaterial_Texture2D_0,View_MaterialTextureBilinearWrapedSampler),Local18);
	MaterialFloat Local21 = MaterialStoreTexSample(Parameters, Local20, 3);
	MaterialFloat Local22 = abs(Parameters.TangentToWorld[2].b);
	MaterialFloat Local23 = lerp((0.00000000 - 1.00000000),(1.00000000 + 1.00000000),Local22);
	MaterialFloat Local24 = saturate(Local23);
	MaterialFloat3 Local25 = lerp(Local16,Local20.rgb,Local24.r.r);
	MaterialFloat Local26 = PositiveClampedPow(Local25.g,Material.PreshaderBuffer[8].x);
	MaterialFloat2 Local27 = Parameters.TexCoords[0].xy;
	MaterialFloat2 Local28 = (DERIV_BASE_VALUE(Local27) * ((MaterialFloat2)Material.PreshaderBuffer[8].y));
	MaterialFloat Local29 = MaterialStoreTexCoordScale(Parameters, DERIV_BASE_VALUE(Local28), 1);
	MaterialFloat4 Local30 = Texture2DSampleBias(Material_Texture2D_1,samplerMaterial_Texture2D_1,DERIV_BASE_VALUE(Local28),View.MaterialTextureMipBias);
	MaterialFloat Local31 = MaterialStoreTexSample(Parameters, Local30, 1);
	MaterialFloat Local32 = lerp(Local26,Local30.r,0.50000000);
	MaterialFloat3 Local33 = lerp(Material.PreshaderBuffer[10].xyz,Material.PreshaderBuffer[9].xyz,Local32);
	MaterialFloat3 Local34 = (Local33 + ((MaterialFloat3)Material.PreshaderBuffer[10].w));
	MaterialFloat3 Local35 = lerp(Local34,Material.PreshaderBuffer[12].xyz,Local30.b);
	MaterialFloat Local36 = (Local32 * Material.PreshaderBuffer[13].y);
	MaterialFloat Local37 = PositiveClampedPow(Local36,Material.PreshaderBuffer[13].z);
	MaterialFloat Local38 = lerp(Material.PreshaderBuffer[13].w,Local37,Local32);
	MaterialFloat Local39 = dot(Parameters.TangentToWorld[2],Parameters.CameraVector);
	MaterialFloat Local40 = saturate(Local39);
	MaterialFloat Local41 = (1.00000000 - Local40);
	MaterialFloat Local42 = PositiveClampedPow(Local41,Material.PreshaderBuffer[14].x);
	MaterialFloat Local43 = lerp(Material.PreshaderBuffer[14].z,Material.PreshaderBuffer[14].y,Local42);
	MaterialFloat Local44 = (Material.PreshaderBuffer[14].w + Local43);
	MaterialFloat Local45 = lerp(Local44,Material.PreshaderBuffer[15].x,Local30.b);
	MaterialFloat Local46 = dot(WorldNormalCopy,Parameters.CameraVector);
	MaterialFloat Local47 = max(0.00000000,Local46);
	MaterialFloat Local48 = (1.00000000 - Local47);
	MaterialFloat Local49 = abs(Local48);
	MaterialFloat Local50 = max(Local49,0.00010000);
	MaterialFloat Local51 = PositiveClampedPow(Local50,5.00000000);
	MaterialFloat Local52 = (Local51 * (1.00000000 - 0.04000000));
	MaterialFloat Local53 = (Local52 + 0.04000000);
	MaterialFloat Local54 = lerp(1.00000000,Material.PreshaderBuffer[15].y,Local53);

	PixelMaterialInputs.EmissiveColor = Local0;
	PixelMaterialInputs.Opacity = Local45;
	PixelMaterialInputs.OpacityMask = 1.00000000;
	PixelMaterialInputs.BaseColor = Local35;
	PixelMaterialInputs.Metallic = Material.PreshaderBuffer[12].w;
	PixelMaterialInputs.Specular = Material.PreshaderBuffer[13].x;
	PixelMaterialInputs.Roughness = Local38;
	PixelMaterialInputs.Anisotropy = 0.00000000;
	PixelMaterialInputs.Normal = MaterialFloat3(0.21223100,0.21223100,1.00000000);
	PixelMaterialInputs.Tangent = MaterialFloat3(1.00000000,0.00000000,0.00000000);
	PixelMaterialInputs.Subsurface = 0;
	PixelMaterialInputs.AmbientOcclusion = 1.00000000;
	PixelMaterialInputs.Refraction = MaterialFloat3(MaterialFloat2(Local54,0.0f),Material.PreshaderBuffer[15].z);
	PixelMaterialInputs.PixelDepthOffset = 0.00000000;
	PixelMaterialInputs.ShadingModel = 1;
	PixelMaterialInputs.FrontMaterial = GetInitialisedStrataData();
	PixelMaterialInputs.SurfaceThickness = 0.01000000;


#if MATERIAL_USES_ANISOTROPY
	Parameters.WorldTangent = CalculateAnisotropyTangent(Parameters, PixelMaterialInputs);
#else
	Parameters.WorldTangent = 0;
#endif
}

